{
  "buildCommand": "pnpm build",
  "outputDirectory": "dist/public",
  "framework": "vite",
  "public": true,
  "rewrites": [
    {
      "source": "/callback",
      "destination": "/api/callback"
    },
    {
      "source": "/logout",
      "destination": "/api/logout"
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
